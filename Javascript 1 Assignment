9.

<!DOCTYPE html>
<html>
    <body>
        <script>
            let arr1 = [1,2,3];
            let arr2 = ['a','b','c'];

            var alternateList = function(arr1,arr2)
            {
                let lengthList = arr1.length + arr2.length;
                let emptyArray = [];
                let j=0,k=0;
                for(let i=0; i<lengthList; i++) {
                    if(i%2==0) {
                    emptyArray[i]=arr1[j++];
                    } else {
                    emptyArray[i]=arr2[k++];
                    }
                }
                return emptyArray;
            }
            document.write(alternateList(arr1,arr2));
        </script>
    </body>
</html>

10.
<!DOCTYPE html>
<html>
    <body>
        <script>
            const number = parseInt(prompt('Enter the number of elements: '));
            let n1 = 0, n2 = 1, next;
            for (let i = 1; i <= number; i++) {
                document.write(n1 + "<br>")
                next = n1 + n2;
                n1 = n2;
                n2 = next;
            }
        </script>
    </body>
</html>

