<!DOCTYPE html>
<html>
    <body>
        <script>
            const n = parseInt(prompt("Enter a Number"));
            var sum=0;
            for(let i=1; i<=n; i++)
            {
                if((i%3==0) || (i%5==0))
                {
                    var sum = sum+i;
                }
            }
            document.write(sum);
        </script>
    </body>
</html>

3.<!DOCTYPE html>
<html>
    <body>
        <script>
            function returnOddEven(n)
            {
                if(n%2==0)
                {
                    return "even"
                }
                else
                {
                    return "odd"
                }
            }
            var n = parseInt(prompt("Enter a number: "));
            document.write(returnOddEven(n))
        </script>
    </body>
</html>

4.<!DOCTYPE html>
<html>
    <body>
        <script>
            function coinFlip(n) 
            {
                var heads = 0;
                const array = Array(n) 
				.fill()
				.map(() => 10 * Math.random()); 
                for(let i=0; i<array.length; i++)
                {
                    if(array[i]<0.95)
                    {
                        array[i] = 0;
                    }
                    else if(array[i]>0.94)
                    {
                        array[i] = 1;
                    }
                    if(array[i] == 1)
                    {
                       heads = heads+1;
                    }
                }
                return heads/n;
            }
            var n = parseInt(prompt("Enter a number: "))
            document.write(coinFlip(n))
        </script>
    </body>
</html>

5.
<!DOCTYPE html>
<html>
    <body>
        <script>
            var year = prompt("please input a year to find out the next 20 years");
            var counter = 20;
            var result=true;
            document.write(leapyear(year,counter,result));
            function leapyear(year, counter, result) 
            {
                while (counter >= 0) 
                {
                    if ((year % 4 === 0) && (year % 100 !== 0)||(year % 400 === 0))
                    {
                        result = true;
                        document.write(year+"\n");
                        year++;
                        counter--;
                    } 
                    else
                    {
                        year++;
                    }    
                }
            }
        </script>
    </body>
</html>

